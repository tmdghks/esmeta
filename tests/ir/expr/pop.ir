@main def main() = {
  let list = (list [4, 5, 3, 7, 5, 2, 3])[#1]
  assert (= list.length 7)
  pop %0 < list
  assert (= %0 4)
  pop list > %1
  assert (= %1 3)
  pop %2 < list
  assert (= %2 5)
  pop list > %3
  assert (= %3 2)
  assert (= list.length 3)
  assert (= list[0] 3)
  assert (= list[1] 7)
  assert (= list[2] 5)
}
